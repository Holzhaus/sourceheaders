{
  "c": {
    "block_comment_end": " */",
    "block_comment_line": " * ",
    "block_comment_start": "/* ",
    "extensions": [
      ".c",
      ".h"
    ],
    "inline_comment": "// "
  },
  "python": {
    "block_comment_end": "\"\"\"",
    "block_comment_start": "\"\"\"\n",
    "extensions": [
      ".py"
    ],
    "inline_comment": "# ",
    "skip_line": "^#!|^# +-\\*-|^# +(?:pylint|pyright|coding|encoding|type|flake8):"
  }
}
